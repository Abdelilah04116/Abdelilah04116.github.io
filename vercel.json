{
  "version": 2,
  "builds": [
    {
      "src": "api/chat_simple.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat_simple.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/chat_simple.py": {
      "maxDuration": 10
    }
  }
} 